{
    "name": "@funpi/vscode",
    "displayName": "FunPi - 放屁",
    "description": "FunPi(放屁) - VSCode扩展",
    "publisher": "chensuiyi",
    "version": "7.1.9",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "type": "module",
    "main": "./dist/extension.cjs",
    "engines": {
        "vscode": "^1.74.0"
    },
    "pricing": "Trial",
    "categories": [
        "Other"
    ],
    "icon": "dist/logo.png",
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://chensuiyi.me",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo"
    },
    "activationEvents": [],
    "scripts": {
        "pack": "vsce package --no-yarn --no-dependencies --no-git-tag-version --allow-unused-files-pattern --allow-missing-repository --out release --baseImagesUrl ./",
        "dev": "yite --command=build --envfile=production --mode=development --workdir=./",
        "build": "yite --command=build --envfile=production --mode=production --workdir=./",
        "dev2": "rollup --config rollup.config.js"
    },
    "files": [
        "dist/**/*",
        "CHANGELOG.md",
        "readme.md",
        "LICENSE.txt"
    ],
    "contributes": {
        "commands": [
            {
                "command": "funPi.helloWorld",
                "title": "Hello World"
            }
        ],
        "configuration": {
            "title": "FunPi",
            "properties": {
                "funPi.message": {
                    "type": "string",
                    "default": "Hello World",
                    "description": "The message to show in the notification"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "funPiExplorer",
                    "title": "FunPi",
                    "icon": "dist/logo.svg"
                }
            ]
        },
        "views": {
            "funPiExplorer": [
                {
                    "id": "funPiWebView",
                    "type": "webview",
                    "name": "FunPi"
                }
            ]
        }
    },
    "gitHead": "cf80a8a4c1f62ffeaa9811c579e4831389c2f9f8",
    "dependencies": {
        "rollup": "^4.27.4"
    }
}
