{
    "name": "fun-pi",
    "displayName": "FunPi - 放屁生态扩展",
    "description": "全栈开发生态funpi(放屁)的VSCode扩展",
    "publisher": "chensuiyi",
    "version": "1.0.0",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "./dist/extension.cjs",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "dist/logo.png",
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://chensuiyi.me",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo"
    },
    "activationEvents": [],
    "scripts": {
        "pack": "vsce package --no-yarn --no-dependencies --no-git-tag-version --allow-unused-files-pattern --allow-missing-repository --out release --baseImagesUrl ./",
        "build": "yite --command=build --envfile=production --mode=production --workdir=./ & rollup --config rollup.config.js"
    },
    "files": [
        "dist/**/*",
        "CHANGELOG.md",
        "readme.md",
        "LICENSE*"
    ],
    "contributes": {
        "commands": [],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "funPiExplorer",
                    "title": "FunPi",
                    "icon": "dist/logo.svg"
                }
            ]
        },
        "views": {
            "funPiExplorer": [
                {
                    "id": "funPiWebView",
                    "type": "webview",
                    "name": "FunPi放屁"
                }
            ]
        }
    },
    "dependencies": {
        "@arco-design/web-vue": "^2.56.3",
        "@arco-plugins/vite-vue": "^1.4.5",
        "@iconify/json": "^2.2.280",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-image": "^3.0.3",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@vscode/codicons": "^0.0.36",
        "@vscode/vsce": "^3.2.1",
        "overlayscrollbars": "^2.10.1",
        "overlayscrollbars-vue": "^0.5.9",
        "pinia": "^2.3.0",
        "sass": "^1.82.0",
        "store2": "^2.14.3",
        "vue": "^3.5.13",
        "vue-i18n": "^10.0.5",
        "vue-router": "^4.5.0",
        "yidash": "^1.12.1",
        "yite-cli": "^3.8.3"
    }
}
